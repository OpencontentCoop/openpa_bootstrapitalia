{literal}
  <style>
      @page {
          margin: 0;
      }

      body {
          margin: 10pt 30pt;
          font-size: 10pt;
          font-family: "Titillium Web", "Arial", "Helvetica", sans-serif;
      }


      h1, h2, h3, h4 {
          font-size: 14pt;
          margin: 0 0 3pt;
          line-height: 1.2;
      }

      h1 {
          font-size: 18pt;
      }

      h2 {
          font-size: 16pt;
      }

      h3 {
          font-size: 13pt;
          font-weight: bold;
      }

      h4 {
          font-size: 11pt;
          font-weight: bold;
      }

      a {
          page-break-inside: avoid
      }

      p {
          margin: 0 0 1pt;
      }

      small {
          font-size: 8pt;
      }

      blockquote {
          page-break-inside: avoid;
      }

      h1, h2, h3, h4, h5, h6 {
          page-break-after: avoid;
          page-break-inside: avoid
      }

      img {
          page-break-inside: avoid;
          page-break-after: avoid;
      }

      tr, pre {
          page-break-inside: avoid
      }

      tr:nth-child(even) {
          background-color: #f2f2f2;
      }

      td {
          padding:10px
      }

      ul, ol, dl {
          page-break-before: avoid
      }

      .m-top {
          margin-top: 20px;
      }

      .p-all {
          padding: 20px;
      }

      .text-center {
          text-align: center;
      }

  </style>
{/literal}
